<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>PunchCMS Admin</title>
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<link rel="stylesheet" type="text/css" href="css/common.css" />
<link rel="stylesheet" type="text/css" href="css/form.css" />
<script type="text/javascript" src="libraries/jquery.js"></script>
<script type="text/javascript" src="libraries/jquery.corner.js"></script>
<script type="text/javascript" src="includes/cms.js"></script>
<script type="text/javascript" src="includes/inc.common.js"></script>
</head>
<body>

<div id="header">
	<div id="topbar">{% if login_username %}Logged in as <span>{{ login_username }}</span> [ <a href="{{ logout_url }}">Log out</a> ]{% else %}&nbsp;{% endif %}</div>
	<h1>PunchCMS Admin Area</h1>
	<ul id="menu-main">
		<li><a href="#" class="active">Accounts</a></li>
	</ul>
	<ul id="menu-sub">
		{% for submenuitem in submenuitems %}
		<li><a href="{{ submenuitem.link }}" class="{SUB_CLASS}">{{ submenuitem.label }}</a></li>
                {% endfor %}
	</ul>
</div>
{% block contents %}    
    
    
    
{% endblock %}    
    
<div id="footer">
	<p>Copyright &copy; 2013 <a href="http://www.felix-it.com">Felix-iT</a></p>
</div>

</body>
</html>